{
  "compilerOptions": {
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2017",
      "es2018.regexp",
      "es2020",
    ],
    "target": "es5",
    "module": "ESNext",
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "alwaysStrict": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src"
  ]
}
